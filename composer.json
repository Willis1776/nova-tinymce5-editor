{
  "name": "willis1776/nova-tinymce5-editor",
  "description": "A Laravel Nova field.",
  "homepage": "https://github.com/Willis1776/nova-tinymce5-editor",
  "keywords": [
    "laravel",
    "nova",
    "tinymce",
    "editor",
    "wysiwyg"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Willis",
      "email": "willis@clarity-code.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Kraftbit\\NovaTinymce5Editor\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kraftbit\\NovaTinymce5Editor\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
